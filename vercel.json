{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 60,
        "memory": 3008
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    }
  ]
}
